{
    "name": "classic-asp-html",
    "scopeName": "text.html.asp",
    "displayName": "Classic ASP w/HTML",
    "description": "Classic ASP Language Support from tmBundle (with html support)",
    "version": "0.0.2",
    "publisher": "jtjoo",
    "engines": {
        "vscode": "^1.4.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jtjoo/vscode-classic-asp-html"
    },
    "bugs": "https://github.com/jtjoo/vscode-classic-asp-html/issues",
    "categories": [
        "Languages",
        "Snippets"
    ],
    "files.associations": {
        "*.asp": "asp",
        "global.asa": "asp"
    },
    "contributes": {
        "languages": [
            {
                "id": "asp",
                "aliases": [
                    "ASP",
                    "asp"
                ],
                "extensions": [
                    ".asa",
                    ".asp"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "asp",
                "scopeName": "text.html.asp",
                "path": "./syntaxes/asp.html.tmLanguage.json",
                "embeddedLanguages": {
                    "text.html": "html",
                    "source.js": "javascript",
                    "source.css": "css"
                }
            }
        ],
        "snippets": [
            {
                "language": "asp",
                "scopeName": "text.html.asp",
                "path": "./snippets/asp.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:asp",
        "onLanguage:html",
        "onLanguage:javascript"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}
